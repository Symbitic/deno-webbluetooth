{
    "lint": {
        "rules": {
            "exclude": [
                "camelcase",
                "no-explicit-any"
            ],
            "include": [
                "explicit-function-return-type",
                "eqeqeq",
                "explicit-module-boundary-types"
            ]
        }
    },
    "tasks": {
        "build": "deno run --allow-env --allow-read --allow-run ./build.ts"
    }
}
